<sumoConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">

    <input>
        <net-file value="../../../../Windows/system32/2024-10-29-20-01-42/osm.net.xml.gz"/>
        <route-files value="../../../../Windows/system32/2024-10-29-20-01-42/osm.bicycle.trips.xml,../../../../Windows/system32/2024-10-29-20-01-42/osm.bus.trips.xml,../../../../Windows/system32/2024-10-29-20-01-42/osm.motorcycle.trips.xml,../../../../Windows/system32/2024-10-29-20-01-42/osm.passenger.trips.xml,../../../../Windows/system32/2024-10-29-20-01-42/osm.pedestrian.rou.xml,../../../../Windows/system32/2024-10-29-20-01-42/osm.rail_urban.trips.xml,../../../../Windows/system32/2024-10-29-20-01-42/osm.truck.trips.xml"/>
        <additional-files value="C:\Users\George Jiang\Desktop\tutorial\additional.xml"/>
    </input>

    
    <output>
        <summary-output value="summary.xml"/>
        <tripinfo-output value="tripinfo.xml"/>
        <fcd-output value="routes.xml"/>
        <collision-output value="collisions.xml"/>
        <stop-output value="stops.xml"/>
    </output>

    <additional>
    	<timedEvent type="SaveTLSStates" source="tls_1" saveDetectors="true" saveConditions="true"/>
    	<timedEvent type="SaveTLSSwitchTimes" source="tls_1"/>
   	<timedEvent type="SaveTLSSwitchStates" source="tls_1"/>
    	<timedEvent type="SaveTLSProgram" source="tls_1"/>
    </additional>


    <processing>
        <ignore-route-errors value="true"/>
        <tls.actuated.jam-threshold value="30"/>
    </processing>

    <routing>
        <device.rerouting.adaptation-steps value="18"/>
        <device.rerouting.adaptation-interval value="10"/>
    </routing>

    <report>
        <verbose value="true"/>
        <duration-log.statistics value="true"/>
        <no-step-log value="true"/>
    </report>

    <gui_only>
        <gui-settings-file value="../../../../Windows/system32/2024-10-29-20-01-42/osm.view.xml"/>
    </gui_only>

</sumoConfiguration>
